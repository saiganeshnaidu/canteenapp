<h2> Start your order on <%= @foodstore.name %> <i class="fa-solid fa-cart-shopping"></i> </h2>

<%= form_with model: @cart do |form| %>
  <div>
    <%= form.hidden_field :user_id, value: Current.user.id %><br>
  </div>
  <div>
    <%= form.hidden_field :food_store_id, value: @foodstore.id %><br>
  </div>
  <div>
    <%= form.submit "Click to order"%>
  </div>
<% end %>