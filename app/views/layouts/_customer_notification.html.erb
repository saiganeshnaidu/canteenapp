<% Notification.all.reverse.each do |x| %>
    <% if x.recipient_type=="NormalCustomer" && x.recipient_id == Current.user.normal_customer.id %> 
      <%= link_to "your order status from " + x.params[:cart].foodstore.name + " restaurant has changed ", cart_lists_path(:cid=> x.params[:cart][:id]) %> <br><br>
    <% end %>
<% end %>