
<h4> Restaurant name </h4>
<%= @foodstore.name %>

<%= form_with model: @cart do |form| %>
    <div>
      <%= form.hidden_field :user_id, value: Current.user.id %>
      <br>
    </div>
    <div>
      <%= form.hidden_field :foodstore_id, value: @foodstore.id %>
      <br>
    </div>
    <div>
      <%= form.submit "Click to cart"%>
    </div>
  <% end %>